<%- include('./partials/header') -%>
    <h1>Home</h1>
    <hr>
    <p>
        <%= homeStartingContent %>
    </p>
    <h1>Blog Posts</h1>
    <hr>
    <% homePosts.forEach((post)=> { %>
        <div class="card">
            <div class="card-header">
                <h2 class="">
                    <%= post.title %>
                </h2>
            </div>
            <div class="card-body">
                <p>
                    <%= post.content.substring(0,100) + "..." %>
                        <a href="/posts/<%= post._id %>">Read More</a>
                </p>
            </div>
        </div>
        <% }) %>
            <%- include('./partials/footer') -%>